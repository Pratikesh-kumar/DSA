/**
 * @param {string} allowed
 * @param {string[]} words
 * @return {number}
 */
var countConsistentStrings = function(allowed, words) {
    let count=0;
   for(let i of words){
       for(let j of i){
           if(!allowed.includes(j)){
               count++;
               break;
           }
       }
   }
   return (words.length)-count;
}
    
